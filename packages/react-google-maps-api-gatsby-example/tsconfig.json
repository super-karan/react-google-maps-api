{
  "extends": "../../tsconfig.json",
  "rootDir": "/src",
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "types": ["googlemaps", "jest", "invariant", "react-dom", "eslint", "estree", "webpack"],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "jsx": "react-jsx",
    "lib": ["dom"],
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "dist",
    "allowJs": true
  }
}
