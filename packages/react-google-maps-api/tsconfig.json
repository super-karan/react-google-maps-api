{
  "extends": "../../tsconfig.json",
  "rootDir": ".",
  "include": ["src/**/*"],
  "exclude": ["node_modules"],
  "types": ["googlemaps", "jest", "invariant", "react-dom", "eslint", "estree", "webpack"],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "target": "es5",
    "lib": ["dom", "es2017"],
    "rootDir": "src",
    "declaration": true,
    "declarationDir": "dist"
  }
}
